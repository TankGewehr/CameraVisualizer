    "version": "0.2.0",
    "configurations": [
        {
            "name":"visualizer(gdb)启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/bin/visualizer",
            "args": [
                "config/config.json"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
